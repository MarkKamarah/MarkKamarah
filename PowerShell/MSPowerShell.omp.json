{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "cache_duration": "none",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f",
            "wsl": "\ue712",
            "wsl_separator": " on "
          },
          "leading_diamond": "\ue0c7\u2593",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "foreground": "#26C6DA",
          "background": "#2f2f2f",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "pwsh": "\uf489 "
            }
          },
          "template": " {{ .Name }} ",
          "foreground": "#ffffff",
          "background": "#0077c2",
          "type": "shell",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "agnoster_full"
          },
          "template": " \ue5ff {{ .Path }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#61AFEF",
          "type": "path",
          "style": "powerline"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
